pipeline{
    agent any

    stages{
        stage("install"){
            agent {docker{image "customnodejs"}
    }
            steps{
                sh "cat /etc/os-release"
                sh "apt update"
                sh "apt install sudo -y"
                sh 'sudo chown -R 995:993 "/.npm"'
                sh "npm install"
            }
        }
    }
}