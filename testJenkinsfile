pipeline{
    agent {docker{image "node:14.18.0"
                args '-u root:sudo'}
    }

    stages{
        stage("install"){
            
            steps{
            //   sh "npm cache clean"
            // sh 'sudo chown -R 995:993 "/.npm"'
                sh "npm install"
            }
        }
        stage("package"){
            steps{
                sh "npm pack"
            }
        }
    }
}